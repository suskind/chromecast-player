{
    "name": "ChromeCastPlayer",
    "version": "0.1.1",
    "description": "Play videos to chromecast. You can control it remotely from your smartphone or another computer.",
    "main": "main.html",
    "window": {
        "transparent": false,
        "title": "ChromeCast Player",
        "toolbar": false,
        "width": 800,
        "height": 300,
        "min_width": 600,
        "min_height": 300,
        "show_in_taskbar": true,
        "resizable": true,
        "exe_icon": "icons/chromecastplayer.png",
        "mac_icon": "icons/chromecastplayer.png",
        "icon": "icons/chromecastplayer.png"
    },
    "maintainers":[ {
        "name": "Claudio Gamboa",
        "email": "gamboa@pdvel.com",
        "web": "http://pdvel.com/" 
    }],
    "contributors": [{
        "name": "Nuno Loureiro",
        "email": "nuno@nloureiro.com",
        "web": "http://www.nloureiro.com/"
    }],
    "dependencies": {
        "castv2-client": "~1.1.0",
        "mdns": "~2.2.10",
        "stream-transcoder": "~0.0.5",
        "socket.io": "~1.3.7"
    }
}
